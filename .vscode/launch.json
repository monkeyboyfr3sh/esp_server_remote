    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "gdbpath": "xtensa-esp32-elf-gdb",
            "name": "Attach to gdbserver",
            "executable": "ssh-client.elf",
            "target": ":3333",
            "debugger_args": ["-ex","tb app_main","-ex","monitor system_reset","-ex","c",],
            "remote": true,
            "cwd": "${workspaceRoot}/build",
            "valuesFormatting": "parseText"
        },

    ]
}